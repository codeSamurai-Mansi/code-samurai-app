<!DOCTYPE html>
<html>
<head> <% include ./head %> </head>
<body>
<% include ./header %>

    <a class="btn" href="/additem">Add New Item</a>

    <div class="card">

    <table class="highlight">
        <tr>
            <th>Song</th><th>Singer</th><th>Added By</th><th>Likes</th><th>Actions</th>
        </tr>

        <% items.forEach(function(item){  %>
        <tr>


            <td>  <%=item.songName%>  </td>
            <td> <%=item.artist%> </td>
            <td> <%=item.addedBy%> </td>
              <td><a href="/like?song=<%=item.songName%>"><i class="material-icons">favorite</i></a></td>
            <td><i class="material-icons">
                delete</i></td>

        </tr>




        <% }) %>
    </table>

    </div>

<% include ./footer %>
</body>
</html>
